  title_font = Sans 14
  title_use_system_font = False
  window_state = maximise
[keybindings]
  close_term = <Primary>w
  go_down = <Primary>Down
  go_left = <Primary>Left
  go_next = <Primary><Shift>Page_Up
  go_prev = <Primary><Shift>Page_Up
  go_right = <Primary>Right
  go_up = <Primary>Up
  resize_down = <Alt>Down
  resize_left = <Alt>Left
  resize_right = <Alt>Right
  resize_up = <Alt>Up
  split_horiz = <Primary><Shift>Down
  split_vert = <Primary><Shift>Right
  toggle_zoom = <Primary>Return
